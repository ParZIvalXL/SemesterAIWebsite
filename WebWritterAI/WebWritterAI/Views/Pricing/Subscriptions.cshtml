@model IEnumerable<DataBase.Models.PricingModel>

<!DOCTYPE html>
<html lang="en" xmlns="http://www.w3.org/1999/html">

<head>
    <meta charset="UTF-8">
    <meta name="author" content="Softnio">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="/docs/images/favicon.png">
    <link rel="stylesheet" href="/copygen-html/html/assets/css/style.css?v1.2.0">
    <title>Pricing - CopyGen - AI Writer &amp; Copywriting Landing Page HTML Template.</title>
</head>

<body class="nk-body" data-menu-collapse="lg">
<div class="nk-app-root ">
    <header class="nk-header">
        @await Html.PartialAsync("Header")
    </header><!-- .nk-header -->
    <main class="nk-pages">
        @await Html.PartialAsync("Pricing", Model, ViewData);
        <section class="section section-bottom-0 section-top-0">
            <div class="container">
                <div class="section-content">
                    <div class="card border-0 shadow-sm rounded-4">
                        <div class="card-body">
                            <h3 class="title fw-medium mb-4">Buy a plan</h3>
                            <form data-action="user/edit" method="post" class="form-submit-init" id="payment-form">
                                <div class="row g-4">
                                    <div class="col-12">
                                        <div class="form-group">
                                            <div class="form-control-wrap">
                                                <label class="form-label" for="card-num">Select a plan</label>
                                                <select name="plan" id="plan" class="form-control form-control-lg">
                                                    @foreach(var plan in Model)
                                                    {
                                                        <option value="@plan.Id">@plan.Name</option>
                                                    }
                                                </select>
                                            </div>
                                        </div>
                                        <!-- .col -->
                                        <div class="col-12">
                                            <div class="form-group">
                                                <div class="form-control-wrap">
                                                    <label class="form-label" for="card-num">Card number</label>
                                                    <input name="card-num" id="card-num" type="number" class="form-control form-control-lg" placeholder="Enter card number">
                                                </div>
                                            </div>
                                        </div>
                                        <!-- .col -->
                                        <div class="col-12">
                                            <div class="form-group">
                                                <div class="form-control-wrap">
                                                    <label class="form-label" for="expiry">Expiry</label>
                                                    <input name="expiry" id="expiry" type="number" class="form-control form-control-lg" placeholder="Expiry">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-12">
                                            <div class="form-group">

                                                <div class="form-control-wrap">
                                                    <label class="form-label" for="cvv">CVV</label>
                                                    <input name="cvv" id="cvv" type="password" class="form-control form-control-lg" placeholder="cvv">
                                                </div>
                                            </div>
                                        </div>
                                        <!-- .col -->
                                        <div class="col-12" id="payment_error" style="display:none">
                                            <p class="text-danger" id="error_text">Error</p>
                                        </div>
                                        <div class="col-12">
                                            <div class="form-group">
                                                <button class="btn btn-primary" type="submit" id="submit-btn">Buy</button>
                                            </div>
                                            <div class="form-result mt-4"></div>
                                        </div>
                                    </div>
                                    <!-- .row -->
                                </div>
                            </form>
                        </div>
                    </div>
                    </div>
            </div><!-- .section-content -->
        </section>
        <section class="section section-bottom-0">
            <div class="container">
                <div class="section-head">
                    <div class="row justify-content-center text-center">
                        <div class="col-xl-8">
                            <h6 class="overline-title text-primary">FAQ'S</h6>
                            <h2 class="title">Frequently Asked Questions</h2>
                        </div>
                    </div>
                </div><!-- .section-head -->
                <div class="section-content">
                    <div class="row g-gs justify-content-center">
                        <div class="col-xl-9 col-xxl-8">
                            <div class="accordion accordion-flush accordion-plus-minus accordion-icon-accent" id="faq-1">
                                <div class="accordion-item">
                                    <h2 class="accordion-header">
                                        <button class="accordion-button" data-bs-toggle="collapse" data-bs-target="#faq-1-1"> What is a copy? </button>
                                    </h2>
                                    <div id="faq-1-1" class="accordion-collapse collapse show" data-bs-parent="#faq-1">
                                        <div class="accordion-body"> Yes, you can write long articel for your blog posts, product descriptions or any long article with CopyGen. We're always updating our template and tools, so let us know what are expecting! </div>
                                    </div>
                                </div><!-- .accordion-item -->
                                <div class="accordion-item">
                                    <h2 class="accordion-header">
                                        <button class="accordion-button collapsed" data-bs-toggle="collapse" data-bs-target="#faq-1-2"> Does CopyGen to write long articles? </button>
                                    </h2>
                                    <div id="faq-1-2" class="accordion-collapse collapse" data-bs-parent="#faq-1">
                                        <div class="accordion-body"> Yes, you can write long articel for your blog posts, product descriptions or any long article with CopyGen. We're always updating our template and tools, so let us know what are expecting! </div>
                                    </div>
                                </div><!-- .accordion-item -->
                                <div class="accordion-item">
                                    <h2 class="accordion-header">
                                        <button class="accordion-button collapsed" data-bs-toggle="collapse" data-bs-target="#faq-1-3"> Is the generated content original? </button>
                                    </h2>
                                    <div id="faq-1-3" class="accordion-collapse collapse" data-bs-parent="#faq-1">
                                        <div class="accordion-body"> Yes, you can write long articel for your blog posts, product descriptions or any long article with CopyGen. We're always updating our template and tools, so let us know what are expecting! </div>
                                    </div>
                                </div><!-- .accordion-item -->
                                <div class="accordion-item">
                                    <h2 class="accordion-header">
                                        <button class="accordion-button collapsed" data-bs-toggle="collapse" data-bs-target="#faq-1-4"> Do you have wordpress plugin? </button>
                                    </h2>
                                    <div id="faq-1-4" class="accordion-collapse collapse" data-bs-parent="#faq-1">
                                        <div class="accordion-body"> Yes, you can write long articel for your blog posts, product descriptions or any long article with CopyGen. We're always updating our template and tools, so let us know what are expecting! </div>
                                    </div>
                                </div><!-- .accordion-item -->
                            </div><!-- .accordion -->
                        </div><!-- .col -->
                    </div><!-- .row -->
                </div><!-- .section-content -->
            </div><!-- .container -->
        </section><!-- .section -->

        @await Html.PartialAsync("BlockWritter");
    </main>
    <footer class="nk-footer">
        @await Html.PartialAsync("Footer");
    </footer>
</div>
@await Html.PartialAsync("Scripts");
<script src="/copygen-html/src/js/buyPlan.js"></script>
<script defer type="module" src="/copygen-html/src/js/unloggedRedirect.js"></script>
</body>

</html>